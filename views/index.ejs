<%- include('header') -%>


  <section class="articles">
    <div class="articles__container">
      <% data.forEach(function(data){ %>
        <div class="articles__items">
          <div class="items__img">
            <img src=<%-data.attributes.image%> alt="">
          </div>
          <h2>
            <%- data.attributes.title %>
          </h2>
          <h2>
            <%- date %>
          </h2>
        </div>

        <% })%>
    </div>
  </section>

  <%- include('footer') -%>
